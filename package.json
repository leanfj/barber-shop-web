{
  "name": "barber-shop-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "devextreme": "23.2.4",
    "devextreme-react": "23.2.4",
    "react": "^18.2.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "sass": "^1.72.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes",
    "test-verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest.config.unit.ts",
    "test:e2e": "npm test -- --watch -c jest.config.e2e.ts",
    "test:infra": "npm test -- --watch -c jest.config.infra.ts",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "eslint:fix": "eslint --quiet --fix",
    "snyk": "snyk test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.93",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "devextreme-cli": "1.6.7",
    "devextreme-themebuilder": "23.2.4",
    "eslint": "^8.57.0",
    "eslint-config-love": "^43.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "snyk": "^1.1286.2",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5"
  }
}